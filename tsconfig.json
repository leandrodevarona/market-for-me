{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@data/*": ["src/lib/data/*"],
      "@auth-astro/*": ["src/lib/auth-astro/*"],
      "@utils/*": ["src/lib/utils/*"],
      "@build/*": ["src/lib/build/*"],
      "@customTypes/*": ["src/lib/customTypes/*"]
    },
    "typeRoots": ["./node_modules/@types", "./src/types.d.ts"]
  }
}
